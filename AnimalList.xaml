<Window x:Class="Система_догляду_за_тваринами.AnimalList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Animal List" Height="450" Width="400">
    <Grid>
        <StackPanel>
            <ListBox x:Name="AnimalsListBox" Margin="10" />
            <Button Content="Пошук тварини" Click="SearchAnimal_Click" Margin="5"/>
            <Button Content="Повернутись у меню" Click="BackToMenu_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
